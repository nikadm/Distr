@using System.ComponentModel
@model IEnumerable<string>

@Html.ActionLink("Home","List","Product")

@foreach (var link in Model)
{
    @Html.RouteLink(link,new
    {
        controller="Product",
        action="List",
        category=link,
        page=1
    },
    new
    {
        @class=link==ViewBag.SeletedCategory ? "selected":null
    })
}